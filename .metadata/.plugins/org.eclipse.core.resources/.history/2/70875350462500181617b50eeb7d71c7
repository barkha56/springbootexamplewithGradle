package sample;

import java.util.Arrays;

public class mainclass {

	public static void main(String[] args) {
		int[] a = new int[4];
		a[0] = 1;
		a[1] = 2;
		a[2] = 3;
		a[3] = 10;

		int[] m = new int[4];
		m[0] = 2;
		m[1] = 3;
		m[2] = 4;
		m[3] = 5;
       int[] ad = new int[2];
       ad[0]= 1234;
       int[] ad1 = new int[2];
       ad[0]= 2345;
		int moves = minimumMoves(ad, ad1);
		System.out.println(moves);

	}

	static int minimumMoves(int[] a, int[] m) {
		int noOfMoves = 0;
		for(int k=0;k<a.length;k++) {
			int length = (int) (Math.log10(a[k]) + 1);			
		    int[] myarr = new int[1];
		    myarr[0]=a[k];
		    int[] myarr1 = new int[1];
		    myarr1[0]=a[k];
		    
		   for (int i = 0; i < length; i++) {				
				if (myarr[i] != m[i]) {
					while (a[i] - m[i] != 0) {
						if (a[i] - m[i] > 0) {

							a[i] = a[i] - 1;
						} else {
							a[i] = a[i] + 1;
						}
						noOfMoves++;
					}

				}
			}
		}
		
		
		
		
		return noOfMoves;

	}

}
