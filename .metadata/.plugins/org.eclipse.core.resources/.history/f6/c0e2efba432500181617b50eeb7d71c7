package sample;

import java.util.HashMap;
import java.util.Map;

public class mainclass {
	
	public static void main(String[] args) {
		int[] a = new int[4];
		a[0]= 1;
		a[1] =2;
		a[2]=3;
		a[4]=4;
		
		int[] m = new int[4];
		m[0]= 2;
		m[1] =3;
		m[2]=4;
		m[4]=5;
		
		minimumMoves(a,m);
		
	}
	
	 static int minimumMoves(int[] a, int[] m) {
	        int noOfMoves = 0;
	        for(int i=0;i<a.length;i++){
	            for(int j=0;j<m.length;j++){
	                if(a[i] != m[j]){
	                    while(a[i]-m[j] !=0) {
	                       if(a[i]-m[j] >0){
	                          
	                        a[i] =a[i]-1;
	                    } else {
	                        a[i] =a[i]+1;
	                    } 
	                        noOfMoves ++;
	                    }
	                   
	                }
	            }
	        }
			return noOfMoves;

	    }

}
